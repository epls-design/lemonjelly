{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "ezpz/image-compare",
	"title": "Image Compare",
	"category": "media",
	"icon": "image-flip-horizontal",
	"description": "Displays two images in a before/after component allowing the user to compare differences.",
	"keywords": ["before", "after", "twentytwenty", "compare", "comparison"],
	"version": "1.0.0",
	"textdomain": "lemonjelly",
	"supports": {
		"align": false,
		"alignText": true,
		"anchor": true,
		"color": {
			"background": true,
			"gradients": false,
			"link": false,
			"text": false
		},
		"customClassName": true,
		"html": false
	},
	"attributes": {
		"outerContainer": {
			"type": "string",
			"default": "image-compare"
		},
		"backgroundColor": {
			"type": "string",
			"default": "white"
		}
	},
	"providesContext": {
		"ezpz/outerContainer": "outerContainer",
		"ezpz/backgroundColor": "backgroundColor"
	},
	"styles": [],
	"viewScript": ["twentytwenty-init"],
	"editorScript": ["twentytwenty-init"],
	"acf": {
		"mode": "preview",
		"renderTemplate": "view.php",
		"postTypes": []
	}
}
